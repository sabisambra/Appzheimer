<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="moviles.appzheimer.DatosBasicosActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Estos datos podran servirte para ubicarte"
        android:id="@+id/textBienvenidaDatosBasicos"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tu nombre es: "
        android:id="@+id/textNombreDatosBasicos"
        android:layout_below="@+id/textBienvenidaDatosBasicos"
        android:layout_alignParentStart="true"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/textNombreUsuarioDatosBasicos"
        android:layout_below="@+id/textNombreDatosBasicos"
        android:layout_alignParentStart="true"
        android:textColor="#f01717"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Y tienes "
        android:id="@+id/textComienzoEdad"
        android:textSize="30dp"
        android:layout_below="@+id/textNombreUsuarioDatosBasicos"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textEdadDatosBasicos"
        android:layout_below="@+id/textNombreUsuarioDatosBasicos"
        android:layout_toEndOf="@+id/textComienzoEdad"
        android:textColor="#f01717"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" AÃ±os"
        android:id="@+id/textView"
        android:layout_below="@+id/textNombreUsuarioDatosBasicos"
        android:layout_toEndOf="@+id/textEdadDatosBasicos"
        android:textAlignment="inherit"
        android:textSize="30dp"
        android:textIsSelectable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="La direccion de tu casa es:"
        android:id="@+id/textView7"
        android:textSize="29dp"
        android:layout_below="@+id/textComienzoEdad"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textDireccionDatosBasicos"
        android:layout_below="@+id/textView7"
        android:layout_alignParentStart="true"
        android:textColor="#f01717"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Y la fecha de hoy es "
        android:id="@+id/textView8"
        android:layout_below="@+id/textDireccionDatosBasicos"
        android:layout_alignParentStart="true"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textDiaFechaDatosBasicos"
        android:textSize="30dp"
        android:textColor="#f01717"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" de "
        android:id="@+id/textView10"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textEdadDatosBasicos"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textMesDatosBasicos"
        android:layout_below="@+id/textView8"
        android:layout_toEndOf="@+id/textView10"
        android:textSize="30dp"
        android:textColor="#f01717"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Volver"
        android:id="@+id/VolverDatosBasicos"
        android:onClick="volverMenuPrinciapl"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/textView8" />
</RelativeLayout>
